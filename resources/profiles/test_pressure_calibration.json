{
    "title": "Test/pressure calibration",
    "author": "Decent",
    "notes": "You will need a portafilter fitted with a pressure gauge that leaks or has a slow flow rate. This profile will slowly rise to 9 bar and hold it. Compare what is on the screen with what your analog pressure gauge tells you. Go to Settings->Machine->Calibrate and enter the held pressure value, and then retest until the pressure until the two agree.",
    "beverage_type": "calibrate",
    "profile_type": "settings_2c",
    "target_weight": 0,
    "target_volume": 0,
    "espresso_temperature": 1.0,
    "temperature_presets": [],
    "maximum_pressure": 0,
    "maximum_flow": 0,
    "minimum_pressure": 0.0,
    "preinfuse_frame_count": 1,
    "mode": "frame_based",
    "steps": [
        {
            "name": "Fast Rise",
            "temperature": 1.0,
            "sensor": "coffee",
            "pump": "pressure",
            "transition": "fast",
            "pressure": 7.0,
            "flow": 0.0,
            "seconds": 20.0,
            "volume": 500,
            "exit_if": true,
            "exit_type": "pressure_over",
            "exit_pressure_over": 7.0,
            "exit_pressure_under": 0,
            "exit_flow_over": 6,
            "exit_flow_under": 0
        },
        {
            "name": "Final Slow Rise",
            "temperature": 1.0,
            "sensor": "coffee",
            "pump": "pressure",
            "transition": "smooth",
            "pressure": 9.0,
            "flow": 0.0,
            "seconds": 3.0,
            "volume": 500,
            "exit_if": false,
            "exit_type": "pressure_over",
            "exit_pressure_over": 11,
            "exit_pressure_under": 0,
            "exit_flow_over": 6,
            "exit_flow_under": 0
        },
        {
            "name": "Hold",
            "temperature": 1.0,
            "sensor": "coffee",
            "pump": "pressure",
            "transition": "fast",
            "pressure": 9.0,
            "flow": 0.0,
            "seconds": 60.0,
            "volume": 500,
            "exit_if": false,
            "exit_type": "pressure_over",
            "exit_pressure_over": 11,
            "exit_pressure_under": 0,
            "exit_flow_over": 6,
            "exit_flow_under": 0
        }
    ]
}
